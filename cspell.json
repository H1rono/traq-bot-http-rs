{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "dictionaryDefinitions": [
        {
            "name": "lib-words",
            "path": "./.cspell-dict/lib-words.txt",
            "addWords": true
        },
        {
            "name": "tool-words",
            "path": "./.cspell-dict/tool-words.txt",
            "addWords": true
        },
        {
            "name": "test-words",
            "path": "./.cspell-dict/test-words.txt",
            "addWords": true
        }
    ],
    "dictionaries": [
        "lib-words",
        "tool-words",
        "test-words"
    ],
    "ignorePaths": [
        "target",
        ".cspell-dict/**",
        ".git"
    ]
}
